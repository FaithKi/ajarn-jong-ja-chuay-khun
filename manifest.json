{
    "manifest_version": 2,
    "name": "ajarn-jong-ja-chuay-khun",
    "version": "0.1",
    "permissions": [
        "storage"
      ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["data/professorList.js", "data/quote.js", "modules/storage.js", "modules/professor.js", "main.js"]
        }
    ],
    
    "background": {
        "scripts": ["data/professorList.js", "modules/storage.js", "background.js"]
    },
    "browser_action": {
        "default_icon": "img/temporary_icon.png",
        "default_title": "a popup will come here."
    }
}
